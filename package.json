    "@typescript-eslint/parser": "^2.22.0",
    "cucumber": "^6.0.5",
    "cucumber-electron": "^2.7.0",
    "electron": "^8.0.3",
    "electron-reloader": "^1.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
